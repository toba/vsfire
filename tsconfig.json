{
   "compilerOptions": {
       "module": "commonjs",
       "target": "es6",
       "outDir": "./out",
       "lib": ["es6", "dom"],
       "sourceMap": true,
       "rootDir": ".",
       "noImplicitAny": true,
       "noImplicitThis": true,
       "alwaysStrict": true,
       "removeComments": true,
       "noUnusedLocals": true,
       "strictNullChecks": false,
       "newLine": "LF",
       "allowJs": false,
       "noUnusedParameters": true,
       "allowSyntheticDefaultImports": false,
       "typeRoots": ["./node_modules/@types"]
   },
   "include": ["./src/extension.ts"],
   "exclude": ["node_modules"]
}