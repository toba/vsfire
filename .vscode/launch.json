{
   "version": "0.2.0",
   "configurations": [
      {
         "name": "Launch Tests",
         "type": "extensionHost",
         "request": "launch",
         "runtimeExecutable": "${execPath}",
         "args": ["--extensionDevelopmentPath=${workspaceFolder}", "--extensionTestsPath=${workspaceFolder}/out/test" ],
         "stopOnEntry": false,
         "sourceMaps": true,
         "outFiles": ["${workspaceFolder}/out/test/**/*.js"],
         "preLaunchTask": "npm"
      }
   ]
}